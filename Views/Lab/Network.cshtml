                </tr>
            </tbody>
            <tfoot>
                <tr>
                    <td style="width: 25vw; text-align:right;">
                        Amount :
                    </td>
                    <td style="width: 5vw;">
                        <form id="amount">
                            <input type="number" id="number" value="1" max="25" min="1">
                        </form>
                    </td>
                    <td style="width: 10vw;">
                        <input form="amount" class="light" type="submit" value="Confirm">
                    </td>
                </tr>
            </tfoot>
        </table>
    </div>
</div>

